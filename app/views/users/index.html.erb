<% @users.each do |user| %>
<div class="row">

  <div class="col-md-1">
    <%= image_tag(user.image_url, size: '75') %>
  </div>
    <div class="col-md-11">
      <h2><%= user.username %> </h2>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
        <p><%= user.bio %></p>
    </div>
</div>

<% end %>
